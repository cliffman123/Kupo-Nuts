{
  "version": 2,
  "buildCommand": "cd my-react-app && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "my-react-app/build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ]
}
